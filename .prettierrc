trailingComma: "es5"
endOfLine: lf
printWidth: 80
useTabs: false
tabWidth: 2
semi: true
bracketSpacing: true
overrides:
  - files: "*.njk"
    options:
      printWidth: 10
  - files: "*.js"
    options:
      singleQuote: true
      jsxSingleQuote: true
      jsxBracketSameLine: true
      spacingbracketSpacing: true
